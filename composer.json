{
    "name": "flat/api",
    "version": "0.8.2",
    "description": "PHP client for the Flat REST API",
    "keywords": [
        "Music",
        "Music notation",
        "MusicXML",
        "MIDI",
        "OpenAPI",
        "Sheet music",
        "Music score"
    ],
    "homepage": "https://github.com/FlatIO/api-client-php",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Flat Team",
            "homepage": "https://github.com/FlatIO/api-client-php/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Flat\\APIClient\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Flat\\APIClient\\" : "test/" }
    }
}
